{
  "kind": "collectionType",
  "collectionName": "books",
  "info": {
    "singularName": "book",
    "pluralName": "books",
    "displayName": "Books",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Description": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "ScriptWriters": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author",
      "inversedBy": "scripts"
    },
    "Artists": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author",
      "inversedBy": "arts"
    },
    "series": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::serie.serie",
      "inversedBy": "books"
    },
    "SeriesVolume": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "PublicationDate": {
      "type": "date",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "PageCount": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "ISBN10": {
      "type": "string",
      "default": "9-9999-9999-9",
      "regex": "^(?:ISBN(?:-10)?:? )?(?=[0-9X]{10}$|(?=(?:[0-9]+[- ]){3})[- 0-9X]{13}$)↵ [0-9]{1,5}[- ]?[0-9]+[-●]?[0-9]+[-●]?[0-9X]$",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "ISBN13": {
      "type": "string",
      "regex": "^(?:ISBN(?:-13)?:? )?(?=[0-9]{13}$|(?=(?:[0-9]+[- ]){4})[- 0-9]{17}$)↵ 97[89][- ]?[0-9]{1,5}[- ]?[0-9]+[- ]?[0-9]+[- ]?[0-9]$",
      "default": "999-9-9999-9999-9",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Genre": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::genre.genre"
    },
    "AgeGroup": {
      "type": "enumeration",
      "enum": [
        "All",
        "Children",
        "Teenagers",
        "Adults"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": "All"
    },
    "Type": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "Franco-belge",
        "Manga",
        "Comics"
      ],
      "default": "Franco-belge"
    }
  }
}
