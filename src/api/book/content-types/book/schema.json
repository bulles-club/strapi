{
  "kind": "collectionType",
  "collectionName": "books",
  "info": {
    "singularName": "book",
    "pluralName": "books",
    "displayName": "Books",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Description": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "ScriptWriters": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author",
      "inversedBy": "scripts"
    },
    "Artists": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author",
      "inversedBy": "arts"
    },
    "SeriesVolume": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "PageCount": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "ISBN10": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "maxLength": 10,
      "minLength": 10
    },
    "ISBN13": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "minLength": 13,
      "maxLength": 13
    },
    "Genre": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::genre.genre"
    },
    "AgeGroup": {
      "type": "enumeration",
      "enum": [
        "All",
        "Children",
        "Teenagers",
        "Adults"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": "All"
    },
    "Type": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "Franco-belge",
        "Manga",
        "Comics"
      ],
      "default": "Franco-belge"
    },
    "PublicationYear": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "integer"
    },
    "Publisher": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::publisher.publisher"
    },
    "Series": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::serie.serie",
      "inversedBy": "Books"
    }
  }
}
